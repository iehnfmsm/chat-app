<div class="message">
  <div class="message-top">
    <div class="message-name">
      <%= message.user.name %>
    </div>
    <div class="message-date">
      <%= message.created_at%>
    </div>
  </div>
  <div class="message-bottom">
    <div class="message-content">
      <%= message.content %>
    </div>
  </div>
</div>